                                                    }

                                                    @if (usertypeid == 3 || usertypeid == 4 || usertypeid == 5)
                                                    {
                                                        <li>
                                                            <a href="@Url.Content("~/Finder/FinderOfDonors")">Pronađi donora</a>
                                                        </li>
                                                    }
