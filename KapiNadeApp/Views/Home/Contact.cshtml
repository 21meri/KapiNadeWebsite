    <script>
        function initMap() {
            var location = { lat: 44.8109, lng: 15.8698 };
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 15,
                center: location
            });
            var marker = new google.maps.Marker({
                position: location,
                map: map
            });
        }
